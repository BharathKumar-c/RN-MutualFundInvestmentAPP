{
  "compilerOptions": {
    "jsx": "react-native",
    "paths": {
      "HalfPieChart": ["./src/HalfPieChart/index"]
    },
    "target": "esnext",
    "declaration": true,
    "outDir": "dist",
    "esModuleInterop": true,
    "allowUnreachableCode": false,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "isolatedModules": true,
    "lib": ["es2017"]
  }
}
